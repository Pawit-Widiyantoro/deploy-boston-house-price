{
    "version": 2,
    "builds": [
      {
        "src": "/api/app.py", 
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/app.py" 
      }
    ],
    "env": {
      "FLASK_ENV": "production",
        "VERCEL_PYTHON_VERSION": "3.11"
    },
    "functions": {
        "api/**/*.py": {
          "runtime": "python3.11"
        }
      }
  }
  